<input type="button" name="next" class="next action-button" value="Skip" />
<input type="button" name="previous" class="previous action-button-previous" value="Back" />
<br><br><br>
<%= form_for @user, as: :user, url: profile_path(@user), html: { multipart: true, autocomplete: 'off', id: "msform", remote: true } do |f| %>
  <%= f.hidden_field :current_setting_step, value: "availability", class: 'form-control' %>
  <div class="form-card">
    <div class="row">
      <div class="col-7">
        <h2 class="fs-title">Availability</h2>
      </div>
    </div>
    <%= render 'errors', {object: @user} %>
    <%= f.hidden_field :is_time_set, value: true %>
    <%= render 'availability_timings', {f: f} %>

    <div class="white-cont clearfix text-center">
      <%= f.submit "Submit & Next", class: 'btn btn-primary step_submit'  %>
    </div>
  </div>
<% end %>
