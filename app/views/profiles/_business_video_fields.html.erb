<div class="nested-fields panel panel-default">
  <div class="panel-body">
    <div class="row">
      <div class="col-md-12">
        <%= f.label :video, class: 'ms-label' %><br />
        <%= f.text_field :video, class: 'form-control', required: true %>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12 text-right">
        <%= link_to_remove_association "Remove Business Video", f, class: 'btn btn-danger btn-bh btn-sm m-t-5' %>
      </div>
    </div>
  </div>
</div>
